{
  "extends": "./paths.json",
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "allowJs": false,
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "outDir": "lib-ts",
    "isolatedModules": false,
    "noEmit": false,
    "emitDeclarationOnly": false,
    "declaration": true,
    "declarationDir": "lib-ts/types"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
