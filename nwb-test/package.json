    "umd",
    "types"
  ],
  "scripts": {
    "build-lib": "yarn build:es && yarn build:cjs && yarn build:types",
    "build:es": "export NODE_ENV=production && npx babel src --config-file ./es.babelrc --out-dir es --extensions .ts,.tsx --delete-dir-on-start",
    "build:cjs": "export NODE_ENV=production && npx babel src --config-file ./cjs.babelrc --out-dir lib --extensions .ts,.tsx --delete-dir-on-start",
    "build:types": "rm -rf types && tsc -p tsconfig.types.json"
  },
